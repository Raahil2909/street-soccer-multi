                            <div class="col d-flex justify-content-center final-score" id="final-score-b"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Button gameover modal -->
    <button id="gameOver" type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#gameOverModal" style="display: none;">Game Over</button>
